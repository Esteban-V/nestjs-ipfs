{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@mfsoftworks/nestjs-ipfs/(.*)": "<rootDir>/../lib/$1",
    "@mfsoftworks/nestjs-ipfs": "<rootDir>/../lib"
  }
}