{
  "name": "@mfsoftworks/nestjs-ipfs",
  "version": "1.0.2",
  "description": "A module for utilizing IPFS p2p filesystem with NestJS",
  "author": "MF Softworks <arranjamesfletcher2012@gmail.com>",
  "license": "MIT",
  "url": "https://github.com/mfsoftworks/nestjs-ipfs#readme",
  "funding": {
    "type": "paypal",
    "url": "https://www.paypal.me/nygmarose"
  },
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.json",
    "format": "prettier --write \"**/*.ts\"",
    "lint": "eslint 'lib/**/*.ts' --fix",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "test:e2e": "jest --config ./tests/jest-e2e.json --runInBand",
    "test:e2e:dev": "jest --config ./tests/jest-e2e.json --runInBand --watch",
    "prerelease": "npm run build",
    "release": "env-cmd release-it"
  },
  "keywords": [
    "nestjs",
    "ipfs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mfsoftworks/nestjs-ipfs"
  },
  "peerDependencies": {
    "@nestjs/common": "^6.0.0",
    "@nestjs/core": "^6.0.0",
    "ipfs": "^0.41.2"
  },
  "dependencies": {
    "ipfs": "^0.41.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/common": "^9.0.11",
    "@nestjs/core": "^9.0.11",
    "@nestjs/platform-express": "^9.0.11",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.0.11",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.7",
    "@types/node": "^18.7.6",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "env-cmd": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.19.1",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "reflect-metadata": "^0.1.13",
    "release-it": "^13.1.1",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "supertest": "^4.0.2",
    "ts-jest": "25.1.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.7.4"
  }
}